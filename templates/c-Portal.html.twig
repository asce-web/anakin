{# class Portal({title, icon, links}) #}
<section class="c-Portal">
  <h1>
    <i class="c-Portal__Icon h-Block glyphicons glyphicons-{{icon}}" role="none" aria-hidden="true"></i>
    <span class="h-Block">{{ title }}</span>
  </h1>
  <ol class="o-List c-Portal__List">
    {{ for (let item in links) }}
      <li class="o-List__Item c-Portal__List__Item"><a href="{{ item.url }}">{{ item.text }}</a></li>
    {{ endfor }}
  </ol>
</section>
